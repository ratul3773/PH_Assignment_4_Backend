model cartItem {
    id        String   @id @default(uuid())
    cartId    String   
    mealId    String
    quantity  Int
    addedAt   DateTime @default(now())
    unitPrice Float
    subtotal  Float
    
    cart Cart @relation(fields: [cartId], references: [cartId])
    meal Meal @relation(fields: [mealId], references: [mealId])

    @@index([cartId])
    @@map("cart_items")
    
}